<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="304.0" prefWidth="560.0" style="-fx-background-color: black;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.AdminMiniEmailLoginController">
   <children>
      <AnchorPane layoutX="2.0" layoutY="-34.0" prefHeight="200.0" prefWidth="200.0">
         <children>
            <AnchorPane layoutX="-2.0" layoutY="34.0" prefHeight="58.0" prefWidth="560.0" style="-fx-background-color: white;">
               <children>
                  <Label alignment="CENTER" layoutY="-1.0" prefHeight="58.0" prefWidth="560.0" style="-fx-background-color: #ADD8E6;" text="Login to Your Mail Account">
                     <font>
                        <Font name="Bookshelf Symbol 7" size="22.0" />
                     </font>
                  </Label>
               </children>
            </AnchorPane>
            <TextField fx:id="emailIDtextField" layoutX="204.0" layoutY="155.0" />
            <Label layoutX="111.0" layoutY="157.0" text="Email ID" textFill="WHITE">
               <font>
                  <Font name="Bookman Old Style" size="18.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
      <PasswordField fx:id="passwordTextField" layoutX="207.0" layoutY="174.0" />
      <Label layoutX="112.0" layoutY="173.0" text="Password" textFill="WHITE">
         <font>
            <Font name="Bookman Old Style" size="18.0" />
         </font>
      </Label>
      <Button fx:id="loginButton" layoutX="207.0" layoutY="218.0" mnemonicParsing="false" onAction="#logintomail" text="Login">
         <font>
            <Font name="Consolas" size="14.0" />
         </font></Button>
      <Button layoutX="297.0" layoutY="218.0" mnemonicParsing="false" onAction="#cancelButton" text="Cancel">
         <font>
            <Font size="14.0" />
         </font></Button>
      <Label fx:id="statusLabel" alignment="CENTER" layoutX="101.0" layoutY="263.0" prefHeight="20.0" prefWidth="361.0" textFill="WHITE">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
